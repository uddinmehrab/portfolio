{
    "version": 2,
    "rewrites": [
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/projects",
        "destination": "/projects.html"
      },
      {
        "source": "/projects/bmisweb",
        "destination": "/projects-bmisweb.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/projects/portfolio",
        "destination": "/projects-portfolio.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }
  
